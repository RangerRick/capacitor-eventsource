{
  "name": "capacitor-eventsource",
  "version": "0.3.0",
  "description": "EventSource (SSE) API for Capacitor",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "author": "Benjamin Reed <github@raccoonfink.com>",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^3.0.2",
    "@capacitor/core": "^3.0.2",
    "@capacitor/ios": "^3.0.2",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-java": "^1.0.0",
    "rimraf": "^4.1.3",
    "rollup": "^2.40.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^3.0.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorEventsource.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:RangerRick/capacitor-eventsource.git"
  },
  "bugs": {
    "url": "git@github.com:RangerRick/capacitor-eventsource.git/issues"
  },
  "scripts": {
